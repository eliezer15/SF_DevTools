{
    "name": "ShareFile DevTools",
    "description": "A set of tools to automate regular tasks in ShareFile development",
    "version":"0.5",
    "permissions": [
        "*://*.sharefiledev.com/*",
        "contextMenus",
        "activeTab",
        "clipboardWrite"
    ],
    "icons":{
        "16": "icon_small.png",
        "48": "icon_medium.png",
        "128": "icon_large.png"
    },
    
    "background":{
        "persistent": false,
        "scripts": [
            "main.js"
         ]
    },
    "page_action": {
        "default_title":"Page Action"
    },
    "manifest_version": 2
}
